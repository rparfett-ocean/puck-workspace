#ifndef __OHOFA10_REGISTERS_H__

#define ADDR_EOF            (0xffff)

typedef struct
{
    uint16_t    address;
    uint8_t     data;
} SENSOR_REGISTER_PAIR_T;

#define SENSOR_ADDR	(0x6c)

const SENSOR_REGISTER_PAIR_T resetReg =         { 0x0103, 0x01 };  // sw reset
const SENSOR_REGISTER_PAIR_T mirrorReg =        { 0x500e, 0x05 };  //
const SENSOR_REGISTER_PAIR_T flipReg =          { 0x500e, 0x05 };  //

